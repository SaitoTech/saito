{
	"useTabs": false,
	"tabWidth": 2,
	"endOfLine": "lf",
	"semi": true,
	"singleQuote": true,
	"printWidth": 120,
	"jsxSingleQuote": false,
	"quoteProps": "as-needed",
	"trailingComma": "none",
	"bracketSpacing": true,
	"arrowParens": "always",
	"insertPragma": false,
	"requirePragma": false,
	"proseWrap": "preserve",
	"htmlWhitespaceSensitivity": "css",
	"vueIndentScriptAndStyle": false,
	"embeddedLanguageFormatting": "auto"
}
